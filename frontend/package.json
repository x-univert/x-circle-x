{
  "name": "xcircle-dapp",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "postinstall": "node postinstall.js",
    "dev": "npm run dev:devnet",
    "dev:devnet": "npm run copy:devnet-config && vite",
    "dev:testnet": "npm run copy:testnet-config && vite",
    "dev:mainnet": "npm run copy:mainnet-config && vite",
    "build": "npm run build:devnet",
    "build:devnet": "npm run copy:devnet-config && vite build",
    "build:testnet": "npm run copy:testnet-config && vite build",
    "build:mainnet": "npm run copy:mainnet-config && vite build",
    "type-check": "tsc -b",
    "copy:devnet-config": "node -e \"require('fs').copyFileSync('./src/config/config.devnet.ts', './src/config/index.ts')\"",
    "copy:testnet-config": "node -e \"require('fs').copyFileSync('./src/config/config.testnet.ts', './src/config/index.ts')\"",
    "copy:mainnet-config": "node -e \"require('fs').copyFileSync('./src/config/config.mainnet.ts', './src/config/index.ts')\"",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "6.5.1",
    "@fortawesome/free-brands-svg-icons": "^6.7.2",
    "@fortawesome/free-regular-svg-icons": "6.5.1",
    "@fortawesome/free-solid-svg-icons": "6.5.1",
    "@fortawesome/react-fontawesome": "0.2.0",
    "@multiversx/sdk-core": "15.1.2",
    "@multiversx/sdk-dapp": "5.3.2",
    "@multiversx/sdk-dapp-ui": "0.1.10",
    "@multiversx/sdk-dapp-utils": "3.0.3",
    "@multiversx/sdk-network-providers": "^2.9.3",
    "axios": "1.10.0",
    "bignumber.js": "^9.x",
    "classnames": "2.3.2",
    "i18next": "^25.6.2",
    "moment": "2.29.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hot-toast": "^2.6.0",
    "react-i18next": "^16.3.3",
    "react-router-dom": "6.16.0",
    "react-tooltip": "^5.30.0",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@rollup/plugin-inject": "^5.0.5",
    "@swc/core": "1.3.90",
    "@swc/jest": "0.2.29",
    "@tailwindcss/cli": "4.0.17",
    "@tailwindcss/postcss": "4.1.3",
    "@tailwindcss/vite": "4.1.1",
    "@types/node": "20.7.1",
    "@types/react": "18.2.23",
    "@types/react-dom": "18.2.14",
    "@types/react-router-dom": "5.3.3",
    "@typescript-eslint/eslint-plugin": "6.7.0",
    "@typescript-eslint/parser": "6.7.0",
    "@vitejs/plugin-basic-ssl": "^1.2.0",
    "@vitejs/plugin-react": "4.1.0",
    "autoprefixer": "10.4.16",
    "eslint": "8.50.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "globrex": "^0.1.2",
    "node-stdlib-browser": "^1.3.1",
    "postcss": "8.4.30",
    "prettier": "3.0.3",
    "sass": "^1.93.2",
    "tailwindcss": "4.0.15",
    "typescript": "5.2.2",
    "vite": "^5.4.11",
    "vite-plugin-node-polyfills": "0.14.1",
    "vite-plugin-svgr": "4.0.0",
    "vite-tsconfig-paths": "4.2.1"
  },
  "pnpm": {
    "overrides": {
      "nth-check": "2.0.1"
    }
  }
}
